    CreateInfoLabel("Players: " .. #Players:GetPlayers())
    
    local ExecutorName = (identifyexecutor and identifyexecutor()) or "Unknown"
    CreateInfoLabel("Executor: " .. ExecutorName)
    
    local PingLabel = CreateInfoLabel("Ping: ...", "Ping")
    local FpsLabel = CreateInfoLabel("FPS: ...", "FPS")
    
    task.spawn(function()
        while PlayerInfo.Parent do
            local ping = math.round(Stats.Network.ServerStatsItem["Data Ping"]:GetValue())
            PingLabel.Text = "Ping: " .. ping .. "ms"
            
            local fps = math.round(workspace:GetRealPhysicsFPS())
            FpsLabel.Text = "FPS: " .. fps
            
            wait(1)
        end
    end)

    local Pages = Instance.new("Frame")
