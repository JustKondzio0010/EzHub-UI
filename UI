    PlayerInfoLayout.Parent = PlayerInfo
    PlayerInfoLayout.SortOrder = Enum.SortOrder.LayoutOrder
    PlayerInfoLayout.Padding = UDim.new(0, 10)
    PlayerInfoLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    
